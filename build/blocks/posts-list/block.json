{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "masud-core/posts-list",
  "version": "0.1.0",
  "title": "Masud: Post List",
  "category": "widgets",
  "icon": "grid-view",
  "description": "Post list block.",
  "example": {},
  "supports": {
    "html": false,
    "align": true,
    "ariaLabel": true,
    "spacing": {
      "padding": true,
      "margin": true
    },
    "customClassName": false
  },
  "attributes": {
    "categories": {
      "type": "array"
    },
    "postsToShow": {
      "type": "number",
      "default": 5
    },
    "order": {
      "type": "string",
      "default": "desc"
    },
    "orderBy": {
      "type": "string",
      "default": "date"
    },
    "titleColor": {
      "type": "string",
      "default": "#101010"
    },
    "titleBackgroundColor": {
      "type": "string",
      "default": "#fff000"
    }
  },
  "textdomain": "masud-core",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}